    </div>
  </div>
</div>
{% block javascripts %}
            {{ encore_entry_script_tags('app') }}
  {{ encore_entry_script_tags("app_js") }}
{% endblock %}
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
